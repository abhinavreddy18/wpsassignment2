<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN" "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">
<web-app id="WebApp_ID">
	<display-name>assignment2.2</display-name>
	<servlet>
		<servlet-name>Authentication</servlet-name>
		<display-name>Authentication</display-name>
		<description></description>
		<servlet-class>servlet.Authentication</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>authentify</servlet-name>
		<display-name>authentify</display-name>
		<description>for authorization</description>
		<servlet-class>servlet.authentify</servlet-class>
		<init-param>
			<param-name>x</param-name>
			<param-value>10</param-value>
			<description></description>
		</init-param>
	</servlet>
	<servlet>
		<servlet-name>Rotutingservlet</servlet-name>
		<display-name>Rotutingservlet</display-name>
		<description></description>
		<servlet-class>servlet.Rotutingservlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Authentication</servlet-name>
		<url-pattern>/Authentication</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>authentify</servlet-name>
		<url-pattern>/authentication</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Rotutingservlet</servlet-name>
		<url-pattern>/common</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
</web-app>
